<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<!-- Make sure the fx:controller is set to the fully qualified name
     of your controller class (e.g. com.example.mars.Home). -->

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="526.0" prefWidth="814.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mars.Home">

   <children>
      <!-- Background image -->
      <ImageView fitHeight="744.0" fitWidth="1076.0" layoutX="-122.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/home.jpeg" />
         </image>
      </ImageView>

      <!-- "Play Game" Button -->
      <Button fx:id="play_game" layoutX="325.0" layoutY="185.0" mnemonicParsing="false" onAction="#onPlayGame" prefHeight="31.0" prefWidth="178.0" text="Play Game">
         <font>
            <Font name="Centaur" size="24.0" />
         </font></Button>
      <Button fx:id="support" layoutX="326.0" layoutY="317.0" mnemonicParsing="false" onAction="#onPlayGame" prefHeight="31.0" prefWidth="178.0" text="Support">
         <font>
            <Font name="Centaur" size="24.0" />
         </font>
      </Button>

      <!-- "Continue" Button -->
      <Button fx:id="settings" layoutX="325.0" layoutY="379.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="178.0" text="Settings">
         <font>
            <Font name="Centaur" size="24.0" />
         </font></Button>
      <Button fx:id="character" layoutX="325.0" layoutY="251.0" mnemonicParsing="false" onAction="#onCharacter" prefHeight="46.0" prefWidth="178.0" text="Character">
         <font>
            <Font name="Centaur" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
